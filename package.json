{
  "name": "diatonic-piano",
  "version": "0.1.0",
  "description": "Interactive SVG piano for React",
  "author": "Vegard Egeland",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:coverage": "vitest run --coverage",
    "check": "biome check .",
    "check:fix": "biome check . --write",
    "lint": "biome lint .",
    "lint:fix": "biome lint . --write",
    "format": "biome format .",
    "format:fix": "biome format . --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vegardege/diatonic-piano.git"
  },
  "bugs": {
    "url": "https://github.com/vegardege/diatonic-piano/issues"
  },
  "homepage": "https://github.com/vegardege/diatonic-piano#readme",
  "peerDependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "dependencies": {
    "kamasi": "^0.3.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3"
  }
}
